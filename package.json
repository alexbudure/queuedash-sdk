{
  "name": "@queuedash/sdk",
  "version": "1.0.0",
  "description": "Queuedash SDK for real-time queue monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "release": "pnpm build && changeset publish",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "keywords": [
    "queue",
    "bullmq",
    "bull",
    "bee-queue",
    "groupmq",
    "monitoring",
    "dashboard"
  ],
  "author": "Alex Budure",
  "license": "MIT",
  "peerDependencies": {
    "bee-queue": ">=1.0.0",
    "bull": ">=4.0.0",
    "bullmq": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "bullmq": {
      "optional": true
    },
    "bull": {
      "optional": true
    },
    "bee-queue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.3"
  }
}
